{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "typecheck": "tsc --noEmit -p tsconfig.strict.json",
    "lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx scripts/run-migration.ts",
    "clean": "rm -rf dist node_modules/.vite .tsbuildinfo",
    "fresh": "npm run clean && npm install && npm run build",
    "logs": "tail -f /tmp/server.log",
    "restart": "pkill -9 -f 'node|tsx' && sleep 2 && npm run dev",
    "ci": "npm run typecheck && npm run lint && npm run format && npm run test && npm run build",
    "prepare": "husky"
  },
